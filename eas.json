{
  "cli": {
    "version": ">= 9.0.3"
  },
  "build": {
    "development": {
      "autoIncrement": true,
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "image": "latest"
      },
      "ios": {
        "image": "latest"
      },
      "channel": "development",
      "env": {
        "API_BASE_URL": "https://maisonaxcess.com/api"
      }
    },
    "preview": {  
      "autoIncrement": true,
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "image": "latest",
        "buildType": "apk"
      },
      "ios": {
        "image": "latest",
        "simulator": true
      },
      "env": {
        "API_BASE_URL": "https://maisonaxcess.com/api"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "image": "latest"
      },
      "ios": {
        "image": "latest"
      },
      "env": {
        "API_BASE_URL": "https://maisonaxcess.com/api"
      }
    }
  },
  "submit": {
    "production": {
      
    }
  }
}
